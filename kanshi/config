output eDP-1 adaptive_sync on

profile standalone {
  output eDP-1 {
    adaptive_sync on
    position 0,0
    enable
  }
  exec xbacklight -set 10
}

profile desk@home {
  output eDP-1 {
    position 3840,345
    adaptive_sync on
  }
  output "Microstep MSI MAG323UPF Unknown" {
    position 0,0
    adaptive_sync on
  }

  exec swaymsg workspace web, move workspace to '"Microstep MSI MAG323UPF Unknown"'
  exec xbacklight -set 0
}
